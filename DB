CREATE TABLE FOODVISOR_REVIEW_REGISTER 
(
  SEQ NUMBER NOT NULL 
, TITLE VARCHAR2(255) NOT NULL 
, ADDRESS VARCHAR2(255) NOT NULL 
, IMAGE VARCHAR2(1000) 
, CONTENT VARCHAR2(1000) NOT NULL 
, WRITER VARCHAR2(30) NOT NULL 
, REGDATE DATE NOT NULL 
, GRADE NUMBER DEFAULT 0 
, REVIEWCNT NUMBER DEFAULT 0 
, LIKECNT NUMBER DEFAULT 0 
, CONSTRAINT FOODVISOR_REVIEW_REGISTER_PK PRIMARY KEY 
  (
    SEQ 
  )
  ENABLE 
);
